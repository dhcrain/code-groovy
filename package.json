{
    "name": "code-groovy",
    "displayName": "code-groovy",
    "description": "Groovy support for VSCode",
    "version": "0.0.2",
    "publisher": "marlon407",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "groovy",
        "grails",
        "micronaut"
    ],
    "icon": "groovy.png",
    "activationEvents": [
        "onCommand:extension.organizeImports",
        "onCommand:extension.organizeDependences"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/marlon407/code-groovy"
    },
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.organizeImports",
                "title": "Organize imports"
            },
            {
                "command": "extension.organizeDependences",
                "title": "Organize dependences"
            }
        ],
        "keybindings": [
            {
                "command": "extension.organizeImports",
                "key": "ctrl+shift+o",
                "mac": "cmd+shift+o",
                "when": ""
            },
            {
                "command": "extension.organizeDependences",
                "key": "ctrl+shift+d",
                "mac": "cmd+shift+d",
                "when": ""
            }
        ],
        "snippets": [
            {
              "language": "groovy",
              "path": "./snippets/groovy.json"
            }
          ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
